{
  "name": "build",
  "version": "1.0.0",
  "description": "packages for the JozeeDesign Website",
  "repository": "github:jakobdellago/jozeeDesign",
  "main": "index.js",
  "scripts": {
    "buildCSS": "node-sass scss/main.scss ../resources/css/main.css",
    "watchCSS": "node-sass scss/main.scss ../resources/css/main.css -w scss/modules",
    "buildJS": "browserify javascript/main.js -o ../resources/javascript/main.js",
    "watchJS": "watchify javascript/main.js -o ../resources/javascript/main.js"
  },
  "author": "Jakob Dellago",
  "license": "ISC",
  "devDependencies": {
    "browserify": "16.x",
    "node-sass": "4.x"
  },
  "dependencies": {
    "html-truncate": "^1.2.2",
    "jquery": "3.x",
    "selectric": "^1.13.0",
    "shufflejs": "^5.2.0",
    "slick-carousel": "1.8.0",
    "watchify": "^3.11.0"
  }
}
